<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Data Driven Example">


	<listeners><!-- This listener implements reportng -->
		<listener class-name="org.uncommons.reportng.HTMLReporter"></listener>
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter"></listener>
		<listener class-name="com.w2a.listeners.CustomListeners"></listener>
	</listeners>

	<test thread-count="5" name="Bank Manager Login Test">
		<classes>
			<class name="com.w2a.testcases.BankManagerLoginTest" />
		</classes>
	</test> <!-- Test -->

	<test thread-count="5" name="Add Customer Login Test">
		<classes>
			<class name="com.w2a.testcases.AddCustomerTest" />
		</classes>
	</test> <!-- Test -->
	
	<test thread-count="5" name="Open Account Test">
		<classes>
			<class name="com.w2a.testcases.OpenAccountTest" />
		</classes>
	</test> <!-- Test -->

</suite> <!-- Suite -->
