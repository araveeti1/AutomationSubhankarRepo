<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Zoho CRM Test Suite">

	<listeners><!-- This listener implements reportng -->
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener
			class-name="org.uncommons.reportng.JUnitXMLReporter" />
		<listener class-name="com.w2a.listeners.CustomListeners"></listener>
	</listeners>

	<test thread-count="5" name="Zoho CRM Login Test">
		<classes>
			<class name="com.w2a.testcases.LoginTest" />
		</classes>
	</test> <!-- Test -->
	
	<test thread-count="5" name="Zoho CRM Create Account Test">
		<classes>
			<class name="com.w2a.testcases.CreateAccountTest" />
		</classes>
	</test> <!-- Test -->
	
</suite> <!-- Suite -->
